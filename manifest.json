{

  "manifest_version": 2,
  "name": "Rocket-Air",
  "version": "1.0",

  "description": "Opens multiple websites in single click. ",

  "icons": {
    "48": "icons/rocket.png",
    "64": "icons/rocket.png",
    "96": "icons/rocket.png"
  },


  "browser_action": {
    "default_icon": "icons/rocket.png",
    "default_title": "Rocket-Air",
    "default_popup": "index.html"

  }

  
}